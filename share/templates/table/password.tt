<tr id="[% css_prefix _ field_name %]_tr" class="[% css_prefix -%]tr [% css_prefix _ field_type %] [% css_prefix _ field_name %]">
    <td id="[% css_prefix _ field_name %]_label_td" class="[% css_prefix -%]label_td [% css_prefix _ field_type %]_label_td"><label class="[% css_prefix -%]label [% css_prefix _ field_type %]_label [% IF !field.editable %] noteditable[% END %]" for="[% css_prefix _ form.name %]_[% field_name %]_input">[% field.display_name %]</label></td>
    <td id="[% css_prefix _ field_name %]_input_td" class="[% css_prefix -%]input_td [% css_prefix _ field_type %]_input_td"><input type="password" id="[% css_prefix _ form.name %]_[% field_name %]_input" class="[% css_prefix %]input [% css_prefix _ field_type %]_input [% css_prefix _ field_name %]_input [% IF !field.editable %] noteditable" disabled="disabled[% END %]" name="[% field_name %]"/>
        [% IF !field.editable && field.value.defined %]
        [% PROCESS hidden.tt %]
        [% END %]
    </td>
</tr>